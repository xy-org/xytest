#import libxy.array;
import libxy.string;
import libxy.stdio;
import xytest;

def main~[EntryPoint{priority=100}]() {
    print("Hello from test runner\n");
    tests := @for (f in $* ~Test()) TestVector{"funcname", f};
    #runTests(tests);
    print(f"Found {tests'len} tests\n");
    for (t in tests) {
        print(f"Running {t.name}... ");
        [t.testCb]();
        print("Success\n");
    }
}
